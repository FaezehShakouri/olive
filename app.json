{
  "expo": {
    "name": "olive",
    "slug": "olive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/olive-logo.png",
    "scheme": "olive",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theponyo.olive"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/olive-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.theponyo.olive"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/olive-logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "66ec17bd-fa63-4e2f-ad57-7c891b8f5939"
      }
    }
  }
}
